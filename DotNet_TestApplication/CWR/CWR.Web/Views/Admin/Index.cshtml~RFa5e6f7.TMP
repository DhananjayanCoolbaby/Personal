            <div class="content-wrapper">
                <div class="content">
                    <div class="panel panel-flat">
                        <div class="panel-body">
                            <div class="alert alert-danger   " ng-hide="alertdanger">
                                <a href="#" class="close" data-dismiss="alert">×</a>
                                <strong>Error!</strong>  <a href="#" data-ng-bind="FailMessage" class="alert-link"></a>
                            </div>
                            <div class="alert alert-success  " ng-hide=" alertsuccess">
                                <a href="#" class="close" data-dismiss="alert">×</a>
                                <strong>Success!</strong> Your <a data-ng-bind="SusMessage" href="#" class="alert-link"></a>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>User Name</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" data-ng-model="ngUsername" id="" type="text">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Name</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" id="" data-ng-model="ngName" type="text">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>JoiningDate</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" type="text" id="start_date" readonly data-ng-model="ngJoiningDate" datepicker="datepicker">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Position</label>
                                </div>
                                <div class="col-lg-8">

                                    <select id="ddlPosition" class="form-control" ng-model="ngPosition" data-ng-init="GetPosition()">
                                        @*ng-options="Position.Name for Position in FetchPositionDetiles">*@
                                        <option disabled selected value="">--Select--</option>
                                        <option ng-repeat="Position in FetchPositionDetiles" value="{{Position.ID}}">{{Position.Name}}</option>
                                    </select>

                                    @*<select class="form-control" data-ng-model="ngPosition" data-ng-init="GetPosition()">
                                            <option disabled selected value="">--Select--</option>
                                            <option data-ng-repeat="Position in FetchPositionDetiles" value="ID">{{Position.Name}}</option>

                                        </select>*@
                                </div>
                            </div>

                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Father Name</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" data-ng-model="ngFatherName" id="" required type="text">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Date of Birth: </label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" type="text" id="end_date" required readonly data-ng-model="ngDOB" datepicker1="datepicker1">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Address</label>
                                </div>
                                <div class="col-lg-8">
                                    <textarea data-ng-model="ngAddress" required class="form-control"></textarea>
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Mobile No: </label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" maxlength="10" required numbers-only data-ng-model="ngMobileNo" id="" type="text">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Gender:</label>
                                </div>
                                <div class="col-lg-8">
                                    <select class="form-control" required data-ng-model="ngGender">
                                        <option disabled selected value="">--Select--</option>
                                        <option value="M">Male</option>
                                        <option value="F">Female</option>

                                    </select>
                                </div>
                            </div>

                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Email Address: </label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" required data-ng-model="ngEmail" id="" type="text">
                                </div>
                            </div>

                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Reporting to :</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" required data-ng-model="ngReporting" id="" type="text">
                                </div>
                            </div>

                            <div class="padzero col-lg-12 m-b-10">
                                <div class="col-lg-2">
                                    <label>State</label>
                                </div>
                                <div class="col-lg-4">

                                    <select id="ddlState" class="form-control" required ng-model="ngState" data-ng-init="GetState()">
                                        <option disabled selected value="">--Select--</option>
                                        <option ng-repeat="state in FetchStateDetiles" value="{{state.ID}}">{{state.Name}}</option>
                                    </select>
                                </div>
                            </div>


                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Postcode:</label>
                                </div>
                                <div class="col-lg-8">
                                    <input type="text" class="form-control" required maxlength="6" name="txtAccountNumber" numbers-only ng-model="ngPostcode">
                                    @*<input class="form-control" data-ng-model="ngPostcode" id="" name="number" min="0" max="99" type="number">*@
                                </div>
                            </div>

                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>User Level:</label>
                                </div>
                                <div class="col-lg-8">
                                    <select class="form-control" required data-ng-model="ngUserLevel">
                                        <option disabled selected value="">--Select--</option>
                                        <option value="l3">Level III</option>
                                        <option value="l2">Level II</option>

                                    </select>
                                </div>
                            </div>

                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>User Password:</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" required data-ng-model="ngPassword" id="" type="text">
                                </div>
                            </div>



                            <div class="padzero col-lg-12 m-b-10">
                                <div class="col-lg-2">

                                </div>
                                <div class="col-lg-4">
                                    <button class="btn danger" type="submit" data-ng-click="SubmitData(Username);">Submit</button>
                                    <button class="btn danger">Cancel</button>
                                </div>
                            </div>


                        </div>
                    </div>
                </div>
            </div>

            <script>
                $(document).ready(function () {
                    $('#slider1').cycle({
                        fx: 'fade', // here change effect to blindX, blindY, blindZ etc
                        speed: 'slow',
                        timeout: 2000
                    });
                    $("#nav-image").hide();
                    $(".nav_icon").click(function () {
                        $("#nav-image").fadeToggle();
                    });
                });
            </script>
        </div>
    </div>






    <div class="col-lg-12 m-b-10 table-responsive">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>User Name</th>
                    <th>Name</th>
                    <th>Joining Date</th>
                </tr>
            </thead>
            <tbody>
                <tr data-ng-repeat=" cwrdata in savedcwrdetailes ">
                    <td> <a ng-href="" ng-click=" rebindingcwrdata(cwrdata.UserName);" style="cursor:pointer">{{cwrdata.UserName}} </a> </td>
                    <td>{{cwrdata.Name}}</td>
                    <td>{{cwrdata.JoiningDate}}</td>
                </tr>

            </tbody>
        </table>
    </div>

</body>
</html>
