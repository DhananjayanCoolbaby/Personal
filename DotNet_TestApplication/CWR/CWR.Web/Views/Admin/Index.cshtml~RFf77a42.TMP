            <div class="content-wrapper">
                <div class="content">
                    <div class="panel panel-flat">
                        <div class="panel-body">
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>User Name</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" data-ng-model="ngUsername" id="" type="text">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Name</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" id="" data-ng-model="ngName" type="text">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Joining Date</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" type="text" id="start_date" readonly data-ng-model="ngJoiningDate" datepicker="datepicker">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Position Title:</label>
                                </div>
                                <div class="col-lg-8">

                                    @*<select class="form-control" ng-model="ngPosition" data-ng-init="GetPosition()"
                                        ng-options="Position.ID as Position.Name for Position in FetchPositionDetiles track by Position.ID"></select>*@

                                    <select class="form-control" data-ng-model="ngPosition" data-ng-init="GetPosition()">
                                        <option disabled selected value="">--Select--</option>
                                        <option data-ng-repeat="Position in FetchPositionDetiles" value="ID">{{Position.ID}}</option>

                                    </select>
                                </div>
                            </div>

                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Father Name</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" data-ng-model="ngFatherName" id="" type="text">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Date of Birth: </label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" type="text" id="end_date" readonly data-ng-model="ngDOB" datepicker1="datepicker1">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Address</label>
                                </div>
                                <div class="col-lg-8">
                                    <textarea data-ng-model="ngAddress" class="form-control"></textarea>
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Mobile No: </label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" data-ng-model="ngMobileNo" id="" type="text">
                                </div>
                            </div>
                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Gender:</label>
                                </div>
                                <div class="col-lg-8">
                                    <select class="form-control" data-ng-model="ngGender">
                                        <option disabled selected value="">--Select--</option>
                                        <option value="M">Male</option>
                                        <option value="F">Female</option>

                                    </select>
                                </div>
                            </div>

                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Email Address: </label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" data-ng-model="ngEmail" id="" type="text">
                                </div>
                            </div>

                            <div class="padzero col-lg-12 m-b-10">
                                <div class="col-lg-2">
                                    <label>State</label>
                                </div>
                                <div class="col-lg-4">
                                    <select class="form-control" data-ng-model="ngState" data-ng-init="GetState()">
                                        <option disabled selected value="">--Select--</option>
                                        <option data-ng-repeat="state in FetchStateDetiles" value="state.id">{{state.Name}}</option>

                                    </select>
                                </div>
                            </div>

                            <div class="padzero col-lg-6 m-b-10">
                                <div class="col-lg-4">
                                    <label>Postcode:</label>
                                </div>
                                <div class="col-lg-8">
                                    <input class="form-control" data-ng-model="ngPostcode" id="" type="text">
                                </div>
                            </div>

                            <div class="padzero col-lg-12 m-b-10">
                                <div class="col-lg-2">

                                </div>
                                <div class="col-lg-4">
                                    <button class="btn danger" type="submit" data-ng-click="SubmitData(Username);">Submit</button>
                                    <button class="btn danger">Cancel</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        $(document).ready(function () {
            $('#slider1').cycle({
                fx: 'fade', // here change effect to blindX, blindY, blindZ etc
                speed: 'slow',
                timeout: 2000
            });
            $("#nav-image").hide();
            $(".nav_icon").click(function () {
                $("#nav-image").fadeToggle();
            });
        });
    </script>
</body>
</html>

