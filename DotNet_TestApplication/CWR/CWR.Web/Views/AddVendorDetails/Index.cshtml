<script type="text/javascript">
    $(document).ready(function () {
        $.ajax({
            type: 'POST',
            url: '@Url.Action("GetGetVendorID", "AddVendorDetails")',

            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                debugger;
                $("#VendorId").val(response);
            }
        });

        $('#Clear').click(function () {
            debugger;
            $("#VendorId").val("");
            $("#VendorContactNO").val("");
            $("#VendorEmail").val("");
            $("#Vendorname").val("");
        })
        $('body').on('click', 'a.showMore', function (event) {

            var trid = $(this).closest('td').attr('id'); // table row ID
            var rowindex = $(this).closest('tr').val();
            var VendorID = {
                VendorID: trid
            }
            $.ajax({
                type: 'POST',
                url: '@Url.Action("FetchDataBasedOnVendorID", "AddVendorDetails")',
                data: JSON.stringify(VendorID),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    debugger;
                    var respoData = response.Data.length;;
                    if (response.Data.length == 1) {
                        debugger;
                        $("#VendorId").val(response.Data[0].VendorContactNO);
                        $("#VendorContactNO").val(response.Data[0].VendorContactNO);
                        $("#VendorEmail").val(response.Data[0].VendorEmail);
                        $("#Vendorname").val(response.Data[0].Vendorname);

                    }
                    else {
                        window.location.href = "Erro/Index";
                    }
                },
                error: function () {
                    alert("Something wrong");
                }
            });
        });

        $('body').on('click', 'i.fa-close', function (event) {
            debugger;
            var trid = $(this).closest('td').attr('id'); // table row ID
            var rowindex = $(this).closest('i').val();

            var VendorID = {
                VendorID: trid
            }
            $.ajax({
                type: 'POST',
                url: '@Url.Action("DeleteVendorDetails", "AddVendorDetails")',
                data: JSON.stringify(VendorID),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    debugger;
                    var respoData = response.Data.length;;
                    if (response.Data.length == 1) {
                        debugger;
                        $("#VendorId").val(response.Data[0].VendorContactNO);
                        $("#VendorContactNO").val(response.Data[0].VendorContactNO);
                        $("#VendorEmail").val(response.Data[0].VendorEmail);
                        $("#Vendorname").val(response.Data[0].Vendorname);

                    }
                    else {
                        window.location.href = "Erro/Index";
                    }
                },
                error: function () {
                    alert("Something wrong");
                }
            });
        });
        $("#SubmitData").click(function () {

            var Vendorname = $("#Vendorname").val();
            var VendorID = $("#VendorId").val();
            var VendorEmail = $("#VendorEmail").val();
            var VendorContactNO = $("#VendorContactNO").val();
            var VendorDetails = {
                Vendorname: Vendorname,
                VendorEmail: VendorEmail,
                Vendorname: VendorContactNO,
                VendorID: VendorID

            }
            debugger;
            $.ajax({
                type: 'POST',
                url: '@Url.Action("SaveVendorData", "AddVendorDetails")',
                data: JSON.stringify(VendorDetails),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {

                    location.reload();


                },
                error: function () {
                    alert("Something wrong");
                }
            });
        })

    });
</script>