                <div class="row-fluid">
                    <div class="span12">
                        <div class="portlet box blue" id="form_wizard_1">
                            <div class="portlet-title">
                                <div class="caption">
                                    <i class="icon-reorder"></i> Form Wizard - <span class="step-title">Step 1 of 2</span>
                                </div>
                                <div class="tools hidden-phone">
                                    <a href="javascript:;" class="collapse"></a>
                                    <a href="#portlet-config" data-toggle="modal" class="config"></a>
                                    <a href="javascript:;" class="reload"></a>
                                    <a href="javascript:;" class="remove"></a>
                                </div>
                            </div>
                            <div class="portlet-body form">
                                <form action="#" class="form-horizontal" id="submit_form">
                                    <div class="form-wizard">
                                        <div class="navbar steps">
                                            <div class="navbar-inner">
                                                <ul class="row-fluid">
                                                    <li class="span3">
                                                        <a href="#tab1" data-toggle="tab" class="step ">
                                                            <span class="number">1</span>
                                                            <span class="desc"><i class="icon-ok"></i> Account Setup</span>
                                                        </a>
                                                    </li>

                                                    <li class="span3">
                                                        <a href="#tab4" data-toggle="tab" class="step">
                                                            <span class="number">2</span>
                                                            <span class="desc"><i class="icon-ok"></i> Confirm</span>
                                                        </a>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                        <div id="bar" class="progress progress-success progress-striped">
                                            <div class="bar"></div>
                                        </div>
                                        <div class="tab-content">
                                            <div class="alert alert-error hide">
                                                <button class="close" data-dismiss="alert"></button>
                                                You have some form errors. Please check below.
                                            </div>
                                            <div class="alert alert-success hide">
                                                <button class="close" data-dismiss="alert"></button>
                                                Your form validation is successful!
                                            </div>
                                            <div class="tab-pane active" id="tab1">
                                                <h3 class="block">Provide Vendor account details</h3>


                                                <div class="control-group">
                                                    <label class="control-label">Vendor name<span class="required">*</span></label>
                                                    <div class="controls">
                                                        <input type="text" class="span6 m-wrap" data-ng-model="ngVendorname" name="Vendorname" />
                                                        <span class="help-inline">Provide Vendor name</span>
                                                    </div>
                                                </div>

                                                <div class="control-group">
                                                    <label class="control-label">Vendor Id<span class="required">*</span></label>
                                                    <div class="controls">
                                                        <input type="text" class="span6 m-wrap" name="VendorId" data-ng-model="ngVendorId" />
                                                        <span class="help-inline">Provide Vendor ID</span>
                                                    </div>
                                                </div>
                                                <div class="control-group">
                                                    <label class="control-label">Vendor Contact Number <span class="required">*</span></label>
                                                    <div class="controls">
                                                        <input type="text" data-ng-model="ngVendorContactNO " class="span6 m-wrap" name="VendorContactNO" />
                                                        <span class="help-inline">Provide Vendor Contact Number</span>
                                                    </div>
                                                </div>
                                                <div class="control-group">
                                                    <label class="control-label">Vendor Email<span class="required">*</span></label>
                                                    <div class="controls">
                                                        <input type="text" class="span6 m-wrap" data-ng-model="ngVendorEmail" name="VendorEmail" />
                                                        <span class="help-inline">Provide Vendor Email</span>
                                                    </div>
                                                </div>
                                            </div>


                                            <div class="tab-pane" id="tab4">
                                                <h4 class="block">Confirm Vendor account</h4>
                                                <h4 class="form-section">Account</h4>
                                                <div class="control-group">
                                                    <label class="control-label">Vendor name:</label>
                                                    <div class="controls">
                                                        <span class="text display-value" data-display="Vendorname"></span>
                                                    </div>
                                                </div>
                                                <div class="control-group">
                                                    <label class="control-label">Vendor Id:</label>
                                                    <div class="controls">
                                                        <span class="text display-value" data-display="VendorId"></span>
                                                    </div>
                                                </div>
                                                <div class="control-group">
                                                    <label class="control-label">Vendor Contact Number :</label>
                                                    <div class="controls">
                                                        <span class="text display-value" data-display="VendorContactNO"></span>
                                                    </div>
                                                </div>


                                                <div class="control-group">
                                                    <label class="control-label">Vendor Email:</label>
                                                    <div class="controls">
                                                        <span class="text display-value" data-display="VendorEmail"></span>
                                                    </div>
                                                </div>


                                                @*<h4 class="form-section">Profile</h4>
                                                <h4 class="form-section">Billing</h4>*@
                                            </div>
                                        </div>
                                        <div class="form-actions clearfix">
                                            <a href="javascript:;" class="btn button-previous">
                                                <i class="m-icon-swapleft"></i> Back
                                            </a>
                                            <a href="javascript:;" class="btn blue button-next">
                                                Continue <i class="m-icon-swapright m-icon-white"></i>
                                            </a>
                                            <a data-ng-click="SubmitData();" class="btn green button-submit">
                                                Submit <i class="m-icon-swapright m-icon-white"></i>
                                            </a>
                                        </div>
                                    </div>


                                </form>
                            </div>
                        </div>
                    </div>
