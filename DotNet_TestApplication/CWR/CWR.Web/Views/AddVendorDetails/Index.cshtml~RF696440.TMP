            <!-- BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM-->
            <div id="portlet-config" class="modal hide">
                <div class="modal-header">
                    <button data-dismiss="modal" class="close" type="button"></button>
                    <p>Saved CWR ID's</p>
                </div>
                <div class="modal-body" data-ng-init="TS_GetsavedVendordetailes()">


                    <table class="table table-striped table-bordered table-hover dataTable">
                        <thead>
                            <tr>
                                <th>User Name</th>
                                <th>Name</th>
                                <th>Joining Date</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr class="gradeX odd" data-ng-repeat=" Vendordata in savedVendordetailes ">
                                <td> <a ng-href="" ng-click=" rebindingVendorData(Vendordata.VendorID);" style="cursor:pointer">{{Vendordata.Vendorname}} </a> </td>
                                <td>{{Vendordata.VendorContactNO}}</td>
                                <td>{{Vendordata.VendorEmail}}</td>
                            </tr>

                        </tbody>
                    </table>
                </div>
