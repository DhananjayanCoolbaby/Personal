@{
    ViewBag.Title = "TimeSheet";
}
@model List<CWR.Entity.TimeSheet>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>Login </title>
    <link href="~/Content/custom-style.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/StyleSheet1.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    @*<script src="~/Scripts/angular.js"></script>*@
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/Module/EmployeeModule.js"></script>
    <script src="~/Scripts/Controller/TimeSheetController.js"></script>
    <script src="~/Scripts/Services/EmployeeServices.js"></script>



    <style>
        .control-label, .input-sm {
            width: 5.6%;
            float: left;
            margin-left: 5px;
            text-align: center;
        }

        .padzero {
            padding: 0px !important;
        }
    </style>
    <script>var urlpro = "@Url.Content("~/TimeSheet")";</script>
</head>


<body class=" pace-done" data-ng-app="EmployeeModule" data-ng-controller="TimeSheetController">
    <div class="pace  pace-inactive">
        <div class="pace-progress" style="transform: translate3d(100%, 0px, 0px);" data-progress-text="100%" data-progress="99">
            <div class="pace-progress-inner"></div>
        </div>
        <div class="pace-activity">
        </div>
    </div>
    <!-- Main navbar -->
    <div class="navbar navbar-default header-highlight">
        <div class="navbar-header">
            <a class="navbar-brand" href="#"></a>

            <ul class="nav navbar-nav visible-xs-block">
                <li><a data-toggle="collapse" data-target="#navbar-mobile"><i class="icon-tree5"></i></a></li>
                <li><a class="sidebar-mobile-main-toggle"><i class="icon-paragraph-justify3"></i></a></li>
            </ul>
        </div>


    </div>
    <div class="page-container" style="min-height:609px">
        <div class="page-content">

            <div class="content-wrapper">
                <div class="content">
                    <div class="panel panel-flat">
                        <div class="panel-body">
                            <div class="padzero col-lg-12">
                                <div class="col-lg-4">
                                    <span>Name:</span>
                                </div>
                                <div class="col-lg-4">
                                    <input class="form-control" id="inputsm" type="text" placeholder="Month">
                                </div>
                                <div class="col-lg-4">
                                    <input class="form-control" id="inputsm" type="text" placeholder="Year">
                                </div>
                            </div>
                            <div class="col-lg-12" style="margin-top:20px;">
                                <div class="col-lg-12">
                                    <label>Legents: X Present: V : L : B: A : W : P : H </label>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="col-lg-12 padzero" data-ng-timelist="@Model.Count">
                                    <div class="col-lg-12 padzero">
                                        <label for="inputPassword" data-ng-repeat="data in FetchDateDetails" id="date_{{data.Date}}" class="control-label">{{data.Date}}<span>-{{data.Days}}</span> </label>

                                    </div>
                                    <div class="col-lg-12 padzero">

                                        <span data-ng-repeat="data in FetchDateDetails" ng-if="data.Days != 'sun' || data.Days != 'Sat'"><input class="form-control input-sm" data-ng-keyup="SaveTime();" type="text"></span>

                                        @*@foreach (var itemDate in Model)
                                        {
                                            //var canDate = @itemDate.Days;
                                            if ((@itemDate.Days != "Sun") && (@itemDate.Days != "Sat"))
                                            {
                                                <span><input class="form-control input-sm" id="@itemDate.Date" data-ng-keyup="SaveTime();" type="text"></span>
                                            }
                                            else
                                            {
                                                <span><input class="form-control input-sm" id="@itemDate.Date" ng-disabled="true" type="text"></span>
                                            }
                                        }*@

                                    </div>
                                </div>

                                <div class="col-lg-12 padzero text-right" style="margin-top:20px;">
                                    <button type="button" class="btn btn-primary" data-ng-click="SubmitTimeSheet();">Primary</button>
                                </div>
                            </div>
                            <div class="col-lg-12" style="margin-top:20px;">
                                <table class="table table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Firstname</th>
                                            <th>Lastname</th>
                                            <th>Email</th>
                                            <th>Date</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>John</td>
                                            <td>Doe</td>
                                            <td>john@example.com</td>
                                            <td>j</td>
                                            <td>View</td>
                                        </tr>
                                        <tr>
                                            <td>Mary</td>
                                            <td>Moe</td>
                                            <td>mary@example.com</td>
                                            <td>j</td>
                                            <td>View</td>
                                        </tr>
                                        <tr>
                                            <td>July</td>
                                            <td>Dooley</td>
                                            <td>july@example.com</td>
                                            <td>j</td>
                                            <td>View</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        //$(document).ready(function () {
        //    $('#slider1').cycle({
        //        fx: 'fade', // here change effect to blindX, blindY, blindZ etc
        //        speed: 'slow',
        //        timeout: 2000
        //    });
        //    $("#nav-image").hide();
        //    $(".nav_icon").click(function () {
        //        $("#nav-image").fadeToggle();
        //    });
        //});
    </script>
</body>
</html>
